"immunizations":[
{% for immunization in immunizations %}
{
  "administrationDate": "{{ immunization.occurrenceDateTime }}",
  "expirationDate": "{{ immunization.expirationDate }}",
  "lotNumber": "{{ immunization.lotNumber }}",
  "reaction": {
	"codings": [
	  {% for reaction in immunization.reaction %}
		{% if reaction.detail.display %}
		{
		  "display": "{{ reaction.detail.display | strip }}"
		}{% unless forloop.last %},{% endunless %}
		{% endif %}
	  {% endfor %}
	]
  },
  "immunization": {
	"codings": [
	  {% for coding in immunization.vaccineCode.coding %}
	  {
		{% include '_codableconcept' coding: coding -%}
	  }{% unless forloop.last %},{% endunless %}
	  {% endfor %}
	],
	"text": "{{ immunization.vaccineCode.text | strip }}"
  },
  "immunizationStatus": {
  "codings": [
    {
      "system": "http://hl7.org/fhir/event-status",
      "code": "{{ immunization.status }}",
      "display": "{{ immunization.status }}"      
    }
  ],
  "text": "{{ immunization.status }}"
},
},
{% endfor %}
],
